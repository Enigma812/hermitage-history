{
  "name": "hermitage-history",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:prod": "ng build --configuration production",
    "deploy": "ng deploy --base-href=/hermitage-history/",
    "watch": "ng build --watch --configuration development",
    "lint": "ng lint",
    "stylelint": "stylelint --fix",
    "test": "ng test",
    "coverage": "http-server -c-1 -o -p 9875 ./coverage",
    "update:angular": "ng update @angular/core @angular/cli --force",
    "update:cdk": "ng update @angular/cdk @angular/material"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "16.0.1",
    "@angular/common": "16.0.1",
    "@angular/compiler": "16.0.1",
    "@angular/core": "16.0.1",
    "@angular/forms": "16.0.1",
    "@angular/platform-browser": "16.0.1",
    "@angular/platform-browser-dynamic": "16.0.1",
    "@angular/router": "16.0.1",
    "@angular/service-worker": "16.0.1",
    "@messageformat/core": "3.1.0",
    "@ng-bootstrap/ng-bootstrap": "14.1.1",
    "@ngx-translate/core": "15.0.0",
    "@ngx-translate/http-loader": "8.0.0",
    "@popperjs/core": "2.11.7",
    "bootstrap": "5.2.3",
    "ngx-translate-messageformat-compiler": "6.3.0",
    "rxjs": "7.8.1",
    "tslib": "2.5.0",
    "zone.js": "0.13.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "16.0.1",
    "@angular-eslint/builder": "16.0.1",
    "@angular-eslint/eslint-plugin": "16.0.1",
    "@angular-eslint/eslint-plugin-template": "16.0.1",
    "@angular-eslint/schematics": "16.0.1",
    "@angular-eslint/template-parser": "16.0.1",
    "@angular/cli": "16.0.1",
    "@angular/compiler-cli": "16.0.1",
    "@angular/localize": "16.0.1",
    "@commitlint/cli": "17.6.3",
    "@commitlint/config-conventional": "17.6.3",
    "@types/jasmine": "4.3.1",
    "@typescript-eslint/eslint-plugin": "5.59.5",
    "@typescript-eslint/parser": "5.59.5",
    "angular-cli-ghpages": "1.0.6",
    "eslint": "8.40.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-import-newlines": "1.3.1",
    "eslint-plugin-jasmine": "4.1.3",
    "eslint-plugin-jsdoc": "44.2.3",
    "eslint-plugin-no-null": "1.0.2",
    "eslint-plugin-prefer-arrow": "1.2.3",
    "eslint-plugin-prettier": "4.2.1",
    "http-server": "14.1.1",
    "husky": "4.3.8",
    "jasmine-core": "5.0.0",
    "karma": "6.4.2",
    "karma-chrome-launcher": "3.2.0",
    "karma-coverage": "2.2.0",
    "karma-jasmine": "5.1.0",
    "karma-jasmine-html-reporter": "2.0.0",
    "karma-junit-reporter": "2.0.1",
    "karma-spec-reporter": "0.0.36",
    "lint-staged": "13.2.2",
    "prettier": "2.8.8",
    "prettier-plugin-organize-attributes": "0.0.5",
    "stylelint": "15.6.1",
    "stylelint-config-rational-order": "0.1.2",
    "stylelint-config-standard-scss": "9.0.0",
    "stylelint-order": "6.0.3",
    "typescript": "5.0.4"
  },
  "stylelint": {
    "extends": [
      "./stylelint.config.js"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix"
    ],
    "*.html": [
      "eslint --fix"
    ],
    "*.scss": [
      "stylelint --fix"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
}
